@model IEnumerable<Projekt.Models.Leczenie>

@{
    ViewBag.Title = "Index";
}

<h2>Lista leczeń</h2>

<p>
    @Html.ActionLink("Dodaj leczenie", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Wizyta.Opis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Opis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lek)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Koszt)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Wizyta.Opis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Opis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Koszt)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.Id }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
